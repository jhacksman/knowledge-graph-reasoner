# Venice.ai API Configuration
VENICE_API_KEY="your-api-key-here"  # Get this from Venice.ai
VENICE_MODEL="deepseek-r1-671b"     # The model we're using
VENICE_BASE_URL="https://api.venice.ai/api/v1"  # Venice.ai API base URL

# Venice.ai Rate Limiting Configuration
VENICE_RATE_LIMIT_CALLS_PER_MINUTE=60  # Maximum API calls per minute
VENICE_RATE_LIMIT_BURST_LIMIT=10       # Maximum burst of requests allowed
VENICE_RATE_LIMIT_RETRY_INTERVAL=1.0   # Time to wait between retries in seconds

# Milvus Vector Database Configuration
MILVUS_URI="http://localhost:19530"  # Default Milvus URI if running locally
MILVUS_TOKEN=""                      # Authentication token (if needed)
MILVUS_DB="default"                  # Database name

# Reasoning Configuration
REASONING_MAX_ITERATIONS=3           # Maximum number of reasoning iterations
REASONING_SIMILARITY_THRESHOLD=0.85  # Threshold for similarity matching
REASONING_MAX_CONTEXT_NODES=5        # Maximum number of context nodes
